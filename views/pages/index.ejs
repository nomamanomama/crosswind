    <!-- SEARCH BAR -->
    <div class="row" id="gm-row">
        <div class="input-field col" id="gm-scearchbar"></div>
        <form class="col s12">
            <div class="container">
                <div class="row">
                    <div class="input-field col s12 align-center">
                        <i class="material-icons prefix">search</i>
                        <!-- Keep the Input attributes so that the input validation works -->
                        <!-- <input 
                            id="gm-input" 
                            type="tel"  -->
                        <input class="validate" id="gm-input-location" type="text" title="City Name, 2-Digit State or Country"
                            pattern="(([0-9]{5})|([\w ]+, [\w]+))">
                        <label for="gm-input-location">City, State/Country</label>
                    </div>
                </div>
            </div>
        </form>
        <!-- GOOGLE MAPS -->
        <div class="container">
            <div id="commentmodal" class="modal">
                <div class="modal-content">
                    <div id="cityBoardName">
            
                    </div>
                    <div class="row" id="cityBoard">
                        <form id="commentForm" class="col s12">
                            <div class="row">
                                <div class="input-field col s6">
                                    <input placeholder="Anonymous" id="commentBy" type="text" data-length="10">
                                    <label for="commentBy">Name</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="input-field col s12">
                                    <textarea id="commentMsg" class="materialize-textarea" data-length="40"></textarea>
                                    <label for="commentMsg">Message</label>
                                </div>
                            </div>
                            <button class="btn waves-effect waves-light" type="submit" name="action">Add
                                <i class="material-icons right">send</i>
                            </button>
                        </form>
                    </div>
                </div>
                <div class="modal-footer">
                    <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat">Cancel</a>
                </div>
            </div>
            <div class="row">
                <div class="col s12">
                    <div id="map">
                    </div>
                </div>
                <div class="col s1">
                    <div id="addMarker-row">
                        <a class="btn-floating btn-large tooltipped modal-trigger" data-position="right" data-delay="50" data-tooltip="Add Your Location" type="submit" id="addMarker" value="foo" href="#commentmodal">
                            <img class="material-icons" id="tiny-sock" src="./assets/images/tiny-sock-38px.png">
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
